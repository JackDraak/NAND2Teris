/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

// a=in.!sel
// b=in.sel
PARTS:
    And(a=in, b=sel, out=b);
    Not(in=sel, out=neqsel);
    And(a=neqsel, b=in, out=a);
}
