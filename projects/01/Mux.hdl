/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

PARTS:
    And(a=a, b=b, out=andab);
    And(a=b, b=sel, out=andbc);
    Not(in=sel, out=neqc);
    And(a=a, b=neqc, out=andaneqc);
    Or(a=andab, b=or1, out=out);
    Or(a=andbc, b=andaneqc, out=or1);
}