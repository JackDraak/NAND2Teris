
/* 4-way multiplexor:
 * out = a if sel == 00
 *       b if sel == 01
 *       c if sel == 10
 *       d if sel == 11
 */

CHIP Mux4Way {
    IN a, b, c, d, sel[2];
    OUT out;

PARTS:
	Mux(a=a, b=c, out=outac, sel=sel[0]);
	Mux(a=b, b=d, out=outbd, sel=sel[0]);
	Mux(a=outac, b=outbd, out=out, sel=sel[1]);
}